{
  "clickupProjection": [
    {
      "list": "orders",
      "updates": [
        {
          "fieldName": "Order 2848 Signed PDF URL",
          "from": "effects.canonicalUpsert.patch.artifacts.order2848SignedPdfR2Key",
          "transform": "r2KeyToDownloadUrl"
        }
      ]
    }
  ],
  "contract": {
    "authority": [
      "R2 Canonical"
    ],
    "governs": "Form — Post-payment — Wet Signed 2848",
    "path": "app/contracts/forms/post-payment/wet-signed-2848.contract.json",
    "source": "Tax Monitor Pro",
    "title": "Tax Monitor Pro — Form Contract — Post-payment — Wet Signed 2848 Submitted",
    "validation": {
      "enumStrict": true,
      "normalizeCheckboxToBoolean": true,
      "rejectUnknownValues": true
    },
    "version": 2
  },
  "delivery": {
    "endpoint": "/forms/post-payment/wet-signed-2848",
    "method": "POST",
    "receiptKeyPattern": "receipts/{source}/{eventId}.json",
    "receiptSource": "form"
  },
  "effects": {
    "canonicalUpsert": {
      "patch": {
        "artifacts": {
          "order2848SignedPdfR2Key": "{payload.wetSigned2848.r2Key}"
        },
        "stepBooleans": {
          "wetSig2848Submitted": true
        }
      },
      "target": "orders/{orderId}.json"
    },
    "dedupeKey": "payload.eventId",
    "receiptAppend": {
      "to": "receipts/{source}/{eventId}.json"
    },
    "writeOrder": [
      "receiptAppend",
      "canonicalUpsert",
      "clickupProjection"
    ],
    "writes": [
      "orders"
    ]
  },
  "note": "This endpoint records receipt + canonical order artifact key for a wet-signed (uploaded) Form 2848 PDF. The Worker is responsible for storing the PDF in R2 and populating payload.wetSigned2848.r2Key (and optional metadata) before canonical upsert + ClickUp projection.",
  "payload": {
    "additionalProperties": false,
    "properties": {
      "accountId": {
        "type": [
          "string",
          "null"
        ],
        "maxLength": 200
      },
      "eventId": {
        "type": "string",
        "minLength": 1
      },
      "orderId": {
        "type": "string",
        "minLength": 1
      },
      "sessionToken": {
        "type": "string",
        "minLength": 1
      },
      "submittedAt": {
        "type": [
          "string",
          "null"
        ],
        "format": "date-time"
      },
      "submitterRole": {
        "type": [
          "string",
          "null"
        ],
        "enum": [
          "client",
          "staff",
          null
        ]
      },
      "wetSigned2848": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "contentType": {
            "type": "string",
            "enum": [
              "application/pdf"
            ]
          },
          "filename": {
            "type": "string",
            "minLength": 4,
            "maxLength": 300
          },
          "r2Key": {
            "type": "string",
            "minLength": 10,
            "maxLength": 500
          },
          "sha256": {
            "type": [
              "string",
              "null"
            ],
            "minLength": 16,
            "maxLength": 128
          },
          "sizeBytes": {
            "type": "number",
            "minimum": 1
          }
        },
        "required": [
          "contentType",
          "filename",
          "r2Key",
          "sizeBytes"
        ]
      }
    },
    "required": [
      "eventId",
      "orderId",
      "sessionToken",
      "wetSigned2848"
    ],
    "type": "object"
  },
  "schema": {
    "name": "tm_form_post_payment_wet_signed_2848",
    "version": 2
  }
}