{
  "contract": {
    "authority": [
      "R2 Canonical"
    ],
    "governs": "Form Ingestion — Wet Signed 2848 (Form 9) → Orders Canonical → ClickUp Projection",
    "path": "app/contracts/forms/post-payment/wet-signed-2848.contract.json",
    "source": "Tax Monitor Pro",
    "title": "Tax Monitor Pro — Form 9 — Wet Signed 2848 Submitted",
    "validation": {
      "enumStrict": true,
      "normalizeCheckboxToBoolean": true,
      "rejectUnknownValues": true
    },
    "version": 1
  },
  "schema": {
    "name": "tm_form_wet_signed_2848",
    "version": 1
  },
  "delivery": {
    "dedupeKey": "eventId",
    "endpoint": "https://api.taxmonitor.pro/forms/post-payment/wet-signed-2848",
    "eventType": "WET_SIGNED_2848_SUBMITTED",
    "method": "POST",
    "receiptKeyPattern": "receipts/{source}/{eventId}.json",
    "receiptSource": "form",
    "writes": [
      "orders"
    ]
  },
  "payload": {
    "required": [
      "accountId",
      "eventId",
      "orderId",
      "submittedAt",
      "submitterRole",
      "wetSigned2848"
    ],
    "properties": {
      "accountId": {
        "type": "string",
        "minLength": 6
      },
      "eventId": {
        "type": "string",
        "minLength": 8
      },
      "orderId": {
        "type": "string",
        "minLength": 6
      },
      "submittedAt": {
        "type": "string",
        "format": "date-time"
      },
      "submitterRole": {
        "type": "string",
        "enum": [
          "client",
          "staff"
        ]
      },
      "wetSigned2848": {
        "type": "object",
        "required": [
          "contentType",
          "filename",
          "r2Key",
          "sizeBytes"
        ],
        "properties": {
          "contentType": {
            "type": "string",
            "enum": [
              "application/pdf"
            ]
          },
          "filename": {
            "type": "string",
            "minLength": 4
          },
          "r2Key": {
            "type": "string",
            "minLength": 10
          },
          "sha256": {
            "type": "string",
            "minLength": 16
          },
          "sizeBytes": {
            "type": "number",
            "minimum": 1
          }
        }
      }
    }
  },
  "effects": {
    "ordersPatch": {
      "artifacts": {
        "order2848SignedPdfR2Key": "{payload.wetSigned2848.r2Key}"
      },
      "stepBooleans": {
        "esign2848Submitted": true
      }
    }
  },
  "clickupProjection": [
    {
      "list": "Orders",
      "updates": [
        {
          "fieldName": "Order 2848 Signed PDF URL",
          "from": "ordersPatch.artifacts.order2848SignedPdfR2Key",
          "transform": "r2KeyToDownloadUrl"
        }
      ]
    }
  ]
}