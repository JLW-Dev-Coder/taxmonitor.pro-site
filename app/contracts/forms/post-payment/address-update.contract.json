{
  "clickupProjection": {
    "list": "Orders",
    "statusOnSuccess": null,
    "updateFields": [
      "address_line_1",
      "address_line_2",
      "city",
      "country",
      "state",
      "zip_code",
      "has_previous_address",
      "previous_address_line_1",
      "previous_address_line_2",
      "previous_city",
      "previous_country",
      "previous_state",
      "previous_zip_code"
    ]
  },
  "contract": {
    "authority": "r2",
    "version": "v1"
  },
  "delivery": {
    "endpoint": "/forms/post-payment/address-update",
    "method": "POST",
    "receiptSource": "form",
    "requiresSessionToken": true
  },
  "effects": {
    "appendReceipt": "receipts/form/{eventId}.json",
    "upsert": "orders/{orderId}.json"
  },
  "payload": {
    "required": [
      "eventId",
      "sessionToken",
      "address_line_1",
      "city",
      "country",
      "state",
      "zip_code",
      "has_previous_address"
    ],
    "optional": [
      "address_line_2",
      "previous_address_line_1",
      "previous_address_line_2",
      "previous_city",
      "previous_country",
      "previous_state",
      "previous_zip_code"
    ]
  },
  "schema": {
    "enumStrict": true,
    "normalizeCheckboxToBoolean": true,
    "rejectUnknownValues": true
  }
}