{
  "clickupProjection": [],
  "contract": {
    "authority": ["R2 Canonical"],
    "governs": "Form — Post-payment — Exit Survey",
    "path": "app/contracts/forms/post-payment/exit-survey.contract.json",
    "source": "Tax Monitor Pro",
    "title": "Tax Monitor Pro — Form Contract — Post-payment — Exit Survey",
    "validation": {
      "enumStrict": true,
      "normalizeCheckboxToBoolean": true,
      "rejectUnknownValues": true
    },
    "version": 1
  },
  "delivery": {
    "endpoint": "/forms/post-payment/exit-survey",
    "method": "POST",
    "receiptKeyPattern": "receipts/{source}/{eventId}.json",
    "receiptSource": "form"
  },
  "effects": {
    "dedupeKey": "payload.eventId",
    "writes": ["orders"],
    "writeOrder": ["receiptAppend", "canonicalUpsert", "clickupProjection"],
    "receiptAppend": {
      "to": "receipts/{source}/{eventId}.json"
    },
    "canonicalUpsert": {
      "target": "orders/{orderId}.json",
      "patch": {
        "stepBooleans": {
          "exitSurveySubmitted": true
        }
      }
    }
  },
  "payload": {
    "required": ["eventId", "orderId", "sessionToken"],
    "type": "object",
    "properties": {
      "Exit_Followup_Allowed": {
        "type": ["string", "null"],
        "enum": ["No", "Yes"]
      },
      "Exit_Improvement_Feedback": {
        "type": ["string", "null"],
        "maxLength": 5000
      },
      "Exit_Positive_Feedback": {
        "type": ["string", "null"],
        "maxLength": 5000
      },
      "Exit_Reason": {
        "type": ["string", "null"],
        "enum": [
          "Chose a different provider",
          "No longer need monitoring",
          "Price / budget concerns",
          "Service completed as intended",
          "Switching to resolution services",
          "Temporary pause",
          "Unsure / other"
        ]
      },
      "Future_Support_Interest": {
        "type": ["string", "null"],
        "enum": [
          "Appeals support",
          "Bookkeeping or cleanup",
          "Compliance-only monitoring",
          "IRS resolution services",
          "Not interested at this time",
          "Unsure / revisit later"
        ]
      },
      "Service_Outcome_Met": {
        "type": ["string", "null"],
        "enum": ["No", "Partially", "Yes"]
      },
      "accountId": {
        "type": ["string", "null"]
      },
      "eventId": {
        "type": "string",
        "minLength": 1
      },
      "orderId": {
        "type": "string",
        "minLength": 1
      },
      "sessionToken": {
        "type": "string",
        "minLength": 1
      }
    },
    "additionalProperties": false
  },
  "schema": {
    "name": "tm_form_post_payment_exit_survey",
    "version": 1
  }
}