{
  "clickupProjection": [],
  "contract": {
    "authority": ["R2 Canonical"],
    "governs": "Form — Post-payment — Welcome",
    "path": "app/contracts/forms/post-payment/welcome.contract.json",
    "source": "Tax Monitor Pro",
    "title": "Tax Monitor Pro — Form Contract — Post-payment — Welcome",
    "validation": {
      "enumStrict": true,
      "normalizeCheckboxToBoolean": true,
      "rejectUnknownValues": true
    },
    "version": 1
  },
  "delivery": {
    "endpoint": "/forms/post-payment/welcome",
    "method": "POST",
    "receiptKeyPattern": "receipts/{source}/{eventId}.json",
    "receiptSource": "form"
  },
  "effects": {
    "dedupeKey": "payload.eventId",
    "writes": ["orders"],
    "writeOrder": ["receiptAppend", "canonicalUpsert", "clickupProjection"],
    "receiptAppend": {
      "to": "receipts/{source}/{eventId}.json"
    },
    "canonicalUpsert": {
      "target": "orders/{orderId}.json",
      "patch": {
        "stepBooleans": {
          "welcomeConfirmed": "{payload.welcomeConfirmed}"
        }
      }
    }
  },
  "payload": {
    "additionalProperties": false,
    "properties": {
      "Tax_Monitoring_Service_Progress_Percent": {
        "type": ["number", "null"]
      },
      "Tax_Monitoring_Service_Progress_Status": {
        "type": ["string", "null"],
        "maxLength": 5000
      },
      "accountId": {
        "type": ["string", "null"]
      },
      "eventId": {
        "type": "string",
        "minLength": 1
      },
      "orderId": {
        "type": "string",
        "minLength": 1
      },
      "sessionToken": {
        "type": "string",
        "minLength": 1
      },
      "welcomeConfirmed": {
        "type": "boolean"
      }
    },
    "required": ["eventId", "orderId", "sessionToken", "welcomeConfirmed"],
    "type": "object"
  },
  "schema": {
    "name": "tm_form_post_payment_welcome",
    "version": 1
  }
}